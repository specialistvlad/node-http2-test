// Severity should be one of the following: 0 = off, 1 = warn, 2 = error
{
  "extends": "eslint-config-airbnb-base",
  "rules": {
    "indent": [2, "tab"],
    "no-tabs": [0],
    "max-len": [2, 120],
    "no-warning-comments": 1,
    "no-console": [0],
    "import/no-extraneous-dependencies": 0,
    "class-methods-use-this": 0
  },
  "env": {
    "node": true,
    "jest": true
  }
}
